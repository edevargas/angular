{
  "root": "apps/nashboard-e2e",
  "sourceRoot": "apps/nashboard-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/nashboard-e2e/cypress.json",
        "devServerTarget": "nashboard:serve:development"
      },
      "configurations": {
        "production": {
          "devServerTarget": "nashboard:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@angular-devkit/build-angular:tslint",
      "options": {
        "tsConfig": ["apps/nashboard-e2e/tsconfig.json"],
        "exclude": ["**/node_modules/**", "!apps/nashboard-e2e/**/*"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["nashboard"]
}
